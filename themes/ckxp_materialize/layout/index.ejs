<div class="row">
        <br>
        <div class="col s12"><H3 class="center-align"><%= config.title %></H3></div>

        <div class="col s8 offset-s2"><span class="flow-text"><%= config.description %></span></div>
</div>

<div class="row">
    <% site.posts.forEach(function(post){ %>

    <div class="col s12 ">
        <div class="card blue-grey darken-1">
        <div class="card-content white-text">
            <span class="card-title"><a href="<%- post.path %>"><%- post.title %></a></span>
            <p><% post.content %> Hello</p>
        </div>
        </div>
    </div>

    <% })%>
</div>

    
    
    


